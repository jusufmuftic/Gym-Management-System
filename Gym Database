-- Ovo vam treba samo prvi put.
/*
create database Gym;
use Gym;
*/

-- Ovo koristite samo kada refreshate bazu. Ne treba vam prvi put.
/*
drop table gym_equipment;
drop table gym_trainer;
drop table subscription_members;
drop table members;
drop table subscription;
drop table plan;
drop table trainer;
drop table employee;
drop table gym;
drop table equipment;
*/

/*1*/
create table trainer(
id int primary key auto_increment,
    first_name varchar(50) NOT NULL,
    last_name varchar(50) NOT NULL,
    gender varchar(50) NOT NULL,
    email varchar(50),
    phone_number varchar(50)
);
/*2*/
create table gym(
    id int primary key auto_increment,
    location varchar(50) NOT NULL
);
/*3*/
create table subscription(
    id int primary key auto_increment,
    starting_date datetime NOT NULL,
    ending_date datetime NOT NULL,
    package_type varchar(20) NOT NULL,
    duration varchar(50),
    price int NOT NULL,
    is_active bool NOT NULL,
    gym_id int,
    foreign key(gym_id)references gym(id)
		on delete cascade
        on update cascade
);
/*4*/
create table employee(
    id int primary key auto_increment,
    first_name varchar(50) NOT NULL,
    last_name varchar(50) NOT NULL,
    username varchar(50) NOT NULL UNIQUE,
    pass varchar(20) NOT NULL,
    email varchar(50) NOT NULL,
    gender varchar(50) NOT NULL,
    phone_number varchar(50) NOT NULL,
    salary int,
    gym_id int,
    foreign key(gym_id)references gym(id)
		on delete cascade
        on update cascade
);
/*5*/
create table plan(
    id int primary key auto_increment,
    plan_name varchar(50),
    price int,
    sessions int,
    trainer_id int,
    foreign key(trainer_id)references trainer(id)
		on delete cascade
        on update cascade
);
/*6*/
create table equipment(
    id int primary key auto_increment,
    name varchar(50)
);
/*7*/
create table gym_trainer(
    gym_id int,
    foreign key(gym_id) references gym(id)
        on delete cascade
        on update cascade,
    trainer_id int,
    foreign key(trainer_id) references trainer(id)
        on delete cascade
        on update cascade
);
/*8*/
create table gym_equipment(
    equipment_id int,
    foreign key(equipment_id) references equipment(id)
        on delete cascade
        on update cascade,
    gym_id int,
    foreign key(gym_id) references gym(id)
        on delete cascade
        on update cascade,
    quantity int
);
/*9*/
create table members(
    id int primary key auto_increment,
    first_name varchar(50) NOT NULL,
    last_name varchar(50) NOT NULL,
    gender varchar(50) NOT NULL,
    email varchar(50),
    phone_number varchar(15),
    place varchar(50),
    comm longtext,
    date_of_birth date,
    discount int,
    plan_id int,
    foreign key(plan_id) references plan(id)
        on delete cascade
        on update cascade
);
/*11*/
create table subscription_members(
    subscription_id int,
    foreign key(subscription_id) references subscription(id)
        on delete cascade
        on update cascade,
    members_id int,
    foreign key(members_id) references members(id)
        on delete cascade
        on update cascade
);

